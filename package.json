{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "del dist && tsc",
    "start": "NODE_ENV=prod DEBUG=xmplaylist* nodemon --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=prod DEBUG=xmplaylist* nodemon --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require test/tshook.js test/*.spec.ts",
    "coverage": "NODE_ENV=test nyc npm run test",
    "lint": "eslint -f codeframe src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint -f codeframe --fix src/**/*.ts test/**/*.ts",
    "setupdb": "NODE_ENV=dev ts-node models/dbinit.ts"
  },
  "dependencies": {
    "@hapi/hapi": "18.3.1",
    "@hapi/hoek": "8.0.2",
    "@hapi/joi": "^15.1.0",
    "@sentry/node": "5.5.0",
    "boom": "7.3.0",
    "date-fns": "2.0.0-beta.2",
    "debug": "4.1.1",
    "delay": "4.3.0",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "good-squeeze": "5.1.0",
    "hapi-sentry": "2.0.2",
    "lodash": "4.17.14",
    "p-forever": "2.1.0",
    "pg": "7.11.0",
    "puppeteer": "1.18.1",
    "redis": "2.8.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "sequelize": "5.10.0"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/debug": "4.1.4",
    "@types/hapi__hapi": "^18.2.5",
    "@types/hapi__hoek": "^6.2.0",
    "@types/hapi__joi": "15.0.2",
    "@types/inquirer": "6.0.3",
    "@types/lodash": "4.14.136",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.2",
    "@types/puppeteer": "1.12.4",
    "@types/redis": "2.8.13",
    "@types/request-promise-native": "1.0.16",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "chai": "4.2.0",
    "del": "^5.0.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.15.0",
    "eslint-plugin-import": "2.18.0",
    "inquirer": "6.4.1",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "supertest": "4.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "xm",
    "sirius",
    "siriusxm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT"
}
