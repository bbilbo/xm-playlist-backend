{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc -p tsconfig.json",
    "start": "NODE_ENV=prod DEBUG=xmplaylist* nodemon --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=prod DEBUG=xmplaylist* nodemon --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require test/tshook.js test/*.spec.ts",
    "coverage": "NODE_ENV=test nyc npm run test",
    "lint": "tslint -p tsconfig.json --format stylish \"src/**/*.ts\"",
    "lint:fix": "tslint -p tsconfig.json --fix --format stylish \"src/**/*.ts\"",
    "setupdb": "NODE_ENV=dev ts-node models/dbinit.ts"
  },
  "dependencies": {
    "boom": "7.3.0",
    "good": "8.1.2",
    "good-console": "7.1.0",
    "good-squeeze": "5.1.0",
    "hapi": "18.0.1",
    "hapi-raven": "7.0.1",
    "hoek": "6.1.2",
    "joi": "14.3.1",
    "date-fns": "2.0.0-alpha.25",
    "debug": "4.1.1",
    "delay": "4.1.0",
    "lodash": "4.17.11",
    "p-forever": "1.0.1",
    "pg": "7.8.0",
    "puppeteer": "1.12.1",
    "raven": "2.6.4",
    "redis": "2.8.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "sequelize": "4.42.0"
  },
  "devDependencies": {
    "@types/boom": "7.2.1",
    "@types/debug": "0.0.31",
    "@types/hapi": "18.0.0",
    "@types/hoek": "4.1.3",
    "@types/inquirer": "0.0.43",
    "@types/joi": "14.3.1",
    "@types/lodash": "4.14.120",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.21",
    "@types/puppeteer": "1.12.0",
    "@types/raven": "2.5.2",
    "@types/redis": "2.8.10",
    "@types/request-promise-native": "1.0.15",
    "@types/sequelize": "4.27.34",
    "@types/supertest": "2.0.7",
    "chai": "4.2.0",
    "inquirer": "6.2.2",
    "mocha": "5.2.0",
    "nock": "10.0.6",
    "nodemon": "1.18.9",
    "nyc": "13.1.0",
    "rimraf": "2.6.3",
    "supertest": "3.4.2",
    "ts-node": "8.0.2",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.13.0",
    "typescript": "3.3.1"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "xm",
    "sirius",
    "siriusxm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT"
}
