{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "DEBUG=xmplaylist:* NODE_ENV=dev pm2 start process.dev.json --no-daemon ",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register ./test/test.js ./test/**/*.spec.js",
    "coverage": "nyc npm test",
    "build": "./node_modules/.bin/babel src -d src -d dist",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty src test; exit 0"
  },
  "keywords": [
    "xm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT",
  "dependencies": {
    "JSONStream": "^1.2.1",
    "async-throttle": "1.0.0",
    "debug": "^2.2.0",
    "kcors": "^2.2.0",
    "koa": "2.0.0-alpha.7",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "lodash": "^4.16.4",
    "moment": "^2.15.2",
    "mongodb": "^2.2.11",
    "request": "^2.76.0",
    "request-promise-native": "^1.0.3",
    "spotify-web-api-node": "^2.3.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.0.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "nock": "^9.0.0",
    "nyc": "^8.3.2",
    "pm2": "^2.0.19",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1"
  },
  "engines": {
    "node": ">=6"
  }
}
