{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "rimraf build; tsc -p .",
    "start": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require test/tshook.js test/*.spec.ts",
    "coverage": "NODE_ENV=test nyc npm run test",
    "lint": "tslint -p tsconfig.json --format stylish \"src/**/*.ts\"",
    "setupdb": "NODE_ENV=dev ts-node models/dbinit.ts"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "catbox": "^10.0.2",
    "catbox-redis": "^4.1.0",
    "good": "^8.1.0",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^17.2.0",
    "hapi-raven": "^7.0.0",
    "hoek": "^5.0.3",
    "joi": "^13.1.2",
    "date-fns": "^2.0.0-alpha.7",
    "debug": "^3.1.0",
    "delay": "^2.0.0",
    "lodash": "^4.17.5",
    "p-forever": "^1.0.1",
    "pg": "7.4.1",
    "puppeteer": "^1.1.1",
    "raven": "^2.4.2",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "4.35.0"
  },
  "devDependencies": {
    "@types/catbox": "^10.0.0",
    "@types/boom": "^7.1.1",
    "@types/hoek": "^4.1.3",
    "@types/hapi": "^17.0.3",
    "@types/joi": "^13.0.5",
    "@types/debug": "^0.0.30",
    "@types/delay": "^2.0.1",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/puppeteer": "^1.0.0",
    "@types/raven": "^2.1.5",
    "@types/redis": "^2.8.5",
    "@types/request-promise-native": "^1.0.13",
    "@types/sequelize": "^4.27.7",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nock": "^9.2.3",
    "nodemon": "^1.17.1",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "xm",
    "sirius",
    "siriusxm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT"
}
