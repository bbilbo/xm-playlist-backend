{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "rimraf build; tsc -p .",
    "start": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --require test/tshook.js test/*.spec.ts",
    "coverage": "NODE_ENV=test nyc npm run test",
    "lint": "tslint \"src/**/*.ts\""
  },
  "dependencies": {
    "chromeless": "^1.2.0",
    "date-fns": "^2.0.0-alpha.1",
    "debug": "^3.0.0",
    "kcors": "^2.2.1",
    "koa": "^2.3.0",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa2-raven": "^4.0.12",
    "lodash": "^4.17.4",
    "pg": "7.1.0",
    "raven": "^2.1.1",
    "redis": "^2.8.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "sequelize": "4.4.2"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/kcors": "^2.2.1",
    "@types/koa": "^2.0.39",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-router": "^7.0.22",
    "@types/lodash": "^4.14.72",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.20",
    "@types/raven": "^2.1.2",
    "@types/redis": "^2.6.0",
    "@types/request-promise-native": "^1.0.6",
    "@types/sequelize": "^4.0.67",
    "@types/supertest": "^2.0.2",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "xm",
    "sirius",
    "siriusxm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT"
}
