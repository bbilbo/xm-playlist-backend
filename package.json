{
  "name": "xm-playlist",
  "version": "1.0.0",
  "description": "bpm playlist tracking",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "rimraf build; tsc -p .",
    "start": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/index.ts",
    "scanner": "NODE_ENV=dev DEBUG=xmplaylist* nodemon --exec 'ts-node' src/scanner.ts",
    "test": "NODE_ENV=test mocha --exit --require test/tshook.js test/*.spec.ts",
    "coverage": "NODE_ENV=test nyc npm run test",
    "lint": "tslint -p tsconfig.json --format stylish \"src/**/*.ts\"",
    "setupdb": "NODE_ENV=dev ts-node models/dbinit.ts"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "catbox": "^10.0.3",
    "catbox-redis": "^4.2.1",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^17.5.4",
    "hapi-raven": "^7.0.0",
    "hoek": "^5.0.4",
    "joi": "^13.6.0",
    "date-fns": "^2.0.0-alpha.11",
    "debug": "^4.0.1",
    "delay": "^4.0.1",
    "lodash": "^4.17.11",
    "p-forever": "^1.0.1",
    "pg": "7.4.3",
    "puppeteer": "^1.8.0",
    "raven": "^2.6.4",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "4.38.1"
  },
  "devDependencies": {
    "@types/catbox": "^10.0.1",
    "@types/boom": "^7.2.0",
    "@types/hoek": "^4.1.3",
    "@types/hapi": "^17.0.19",
    "@types/joi": "^13.4.5",
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "10.10.1",
    "@types/puppeteer": "^1.6.4",
    "@types/raven": "^2.5.1",
    "@types/redis": "^2.8.6",
    "@types/request-promise-native": "^1.0.15",
    "@types/sequelize": "^4.27.25",
    "@types/supertest": "^2.0.6",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "require": [
      "./test/tshook.js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": []
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "xm",
    "sirius",
    "siriusxm",
    "radio",
    "recentlyplayed"
  ],
  "author": "scttcper",
  "license": "MIT"
}
